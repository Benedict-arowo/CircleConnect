generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String       @id @default(uuid())
  google_id String? @unique
  github_id String? @unique
  password String?
  first_name String 
  last_name String?
  email String @unique
  profile_picture String?
  joined DateTime @updatedAt()
  last_login DateTime?
}

model Session {
  sid        String   @unique @id
  sess       Json
  expire     DateTime
}
